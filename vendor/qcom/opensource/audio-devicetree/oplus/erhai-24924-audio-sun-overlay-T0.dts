		si,is_digital_pa = <1>;
		sipa_min_range = <5800>;
		sipa_max_range = <7800>;
		sipa_default_mohm = <6800>;
		sipa_min_freq = <100>;
		sipa_max_freq = <2000>;
		is_use_freq = <1>;

		channel_num = <3>;
		channel_num_max = <8>;
	};

	si_pa_Five: si_pa_Five {
		compatible = "si,sia91xx";
		si,si_pa_type = "sia9196";
		si,si_pa_disable_pin = <0>;
