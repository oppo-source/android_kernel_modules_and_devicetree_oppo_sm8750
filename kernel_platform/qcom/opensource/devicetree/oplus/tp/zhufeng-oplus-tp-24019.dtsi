				pins = "gpio65";
				drive-strength = <2>;
				bias-pull-up;
			};
		};
	};

	pmx_csa37f71_interrupt_input {
		csa37f71_interrupt_input: csa37f71_interrupt_input {
			mux {
				pins = "gpio56";
				function = "gpio";
			};

			config {
				pins = "gpio56";
				drive-strength = <2>;
				input-enable;
				bias-disable;
			};
		};
	};
};

&soc {
	devinfo_zhufeng {
		compatible = "oplus-devinfo";
		pinctrl-names = "default", "sleep", "active", "idle";
		pinctrl-0 = <&hw_pins_default>;
		pinctrl-1 = <&aboard_pin_gpio173_sleep>;
		pinctrl-2 = <&aboard_pin_gpio173_active>;
		pinctrl-3 = <&aboard_pin_gpio173_idle>;

		main-sub {
			compatible = "hw-match, main-sub";
			use_tristate_gpio;
			aboard-gpio0 = <&tlmm 173 0>;
			aboard-patterns = <0 0>;	/*0[high-resistance] 1[pull-down] 2[pull-up]*/
			/*main board*/
			match-projects = <24018 24019>;
		};
	};
};

&tlmm {
	hw_pins_default:hw_pins_default {
	};
	aboard_pin_gpio173_sleep:aboard_pin_gpio173_sleep {
		mux {
			pins = "gpio173";
			function = "gpio";
		};
		config {
			pins = "gpio173";
			drive-strength = <2>;
			bias-pull-down;
			input-enable;
		};
	};

	aboard_pin_gpio173_active:aboard_pin_gpio173_active {
		mux {
			pins = "gpio173";
			function = "gpio";
		};
		config {
			pins = "gpio173";
			drive-strength = <2>;
			bias-pull-up;
			input-enable;
		};
	};

	aboard_pin_gpio173_idle:aboard_pin_gpio173_idle {
		mux {
			pins = "gpio173";
			function = "gpio";
		};
		config {
			pins = "gpio173";
			bias-disable;
			input-enable;
		};
	};
};
