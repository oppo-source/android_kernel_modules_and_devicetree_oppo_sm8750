		si,si_pa_reset = <&tlmm 164 0>;
		pinctrl-names = "si_pa_shared_rst_gpio";
		pinctrl-0 = <&sia91xx_gpio_rst>;
		si,is_digital_pa = <1>;
		sipa_min_range = <5800>;
		sipa_max_range = <7800>;
		sipa_default_mohm = <6800>;
		sipa_min_freq = <100>;
		sipa_max_freq = <2000>;
		is_use_freq = <1>;

