&qupv3_hub_i2c4 {
	status = "ok";
	qcom,clk-freq-out = <400000>;
	sipa_i2c_Two: sipa_i2c_Two@6c{
		compatible = "si,sia91xx-i2c";
		reg = <0x6c>;
		si,sipa-dev = <&si_pa_Two>;
		sound-name-prefix = "SpkrTwo";
		status = "ok";
	};
	sipa_i2c_Four: sipa_i2c_Four@6d{
		compatible = "si,sia91xx-i2c";
		reg = <0x6d>;
		si,sipa-dev = <&si_pa_Four>;
