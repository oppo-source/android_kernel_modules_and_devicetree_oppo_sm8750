		compatible = "si,sia91xx";
		si,si_pa_type = "sia9196";
		si,si_pa_disable_pin = <0>;
		oplus,shared_rst_pin = <1>;
		si,si_pa_reset = <&tlmm 164 0>;
		pinctrl-names = "si_pa_shared_rst_gpio";
		pinctrl-0 = <&sia91xx_gpio_rst>;
		si,is_digital_pa = <1>;
		sipa_min_range = <5800>;
